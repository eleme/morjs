{
  "packages": [
    "packages/*",
    "examples/*"
  ],
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md"
  ],
  "npmClient": "pnpm",
  "command": {
    "version": {
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): publish packages",
      "private": false
    },
    "publish": {
      "allowBranch": [
        "main",
        "beta"
      ],
      "message": "chore(release): publish packages"
    }
  },
  "version": "1.0.72"
}
